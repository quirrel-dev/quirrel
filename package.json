    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "@types/websocket": "1.0.5",
    "@types/ws": "8.5.5",
    "delay": "5.0.0",
    "jest": "29.2.2",
    "json-schema-to-typescript": "11.0.2",
    "npm-run-all": "4.1.5",
    "supertest": "6.3.1",
    "ts-jest": "29.0.3",
    "typescript": "4.9.5",
    "websocket": "1.0.34"
  },
