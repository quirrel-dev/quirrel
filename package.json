    "@types/ioredis": "4.27.8",
    "@types/jest": "27.0.2",
    "@types/js-yaml": "4.0.4",
    "@types/jsonwebtoken": "8.5.5",
    "@types/ms": "0.7.31",
    "@types/node-fetch": "2.5.12",
    "@types/parse-gitignore": "1.0.0",
    "@types/redis": "2.8.32",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.1",
    "@types/websocket": "1.0.4",
    "@types/ws": "8.2.0",
    "delay": "5.0.0",
    "jest": "27.3.1",
    "json-schema-to-typescript": "10.1.5",
    "npm-run-all": "4.1.5",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4",
    "websocket": "1.0.34"
  },
  "dependencies": {
    "@babel/parser": "^7.14.7",
    "@babel/traverse": "^7.14.7",
    "@quirrel/owl": "^0.14.3",
    "@sentry/node": "6.14.0",
    "@sentry/tracing": "6.14.0",
    "basic-auth": "2.0.1",
    "body-parser": "1.19.0",
    "chalk": "5.0.0",
    "chokidar": "3.5.2",
    "commander": "^8.0.0",
    "conditional-type-checks": "1.0.5",
    "connect": "3.7.0",
    "cron-parser": "4.1.0",
    "cross-fetch": "^3.1.4",
    "cross-spawn": "7.0.3",
    "dd-trace": "^1.0.0",
    "easy-table": "1.2.0",
    "expand-tilde": "2.0.2",
    "fast-glob": "3.2.7",
    "fastify": "3.22.1",
    "fastify-basic-auth": "2.1.0",
    "fastify-blipp": "3.1.0",
    "fastify-cors": "6.0.2",
    "fastify-plugin": "3.0.0",
    "fastify-static": "^4.2.2",
    "fastify-swagger": "^4.5.0",
    "fastify-websocket": "4.0.0",
    "ioredis": "4.28.0",
