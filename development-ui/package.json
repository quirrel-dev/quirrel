    "@tailwindui/react": "^0.1.1",
    "@types/js-cookie": "^3.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.5",
    "autoprefixer": "^10.2.5",
    "clsx": "^1.1.1",
    "delay": "^5.0.0",
    "immer": "^9.0.1",
    "js-cookie": "^3.0.0",
    "lodash": "^4.17.21",
    "parcel": "^2.0.0-beta.3.1",
    "postcss": "^8.2.8",
    "quirrel": "file:..",
    "react": "17.0.2",
