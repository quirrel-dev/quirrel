    "@types/chai": "4.3.3",
    "@types/ioredis": "4.28.10",
    "@types/jest": "29.2.0",
    "babel-plugin-istanbul": "6.1.1",
    "chai": "4.3.6",
    "ioredis-mock": "5.9.1",
    "jest-playwright-preset": "1.7.2",
    "nyc": "15.1.0",
    "playwright": "1.22.0",
    "serve": "13.0.2",
    "start-server-and-test": "1.14.0"
  },
