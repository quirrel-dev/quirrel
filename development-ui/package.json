    "@types/js-cookie": "^3.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^16.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.2.5",
    "clsx": "^1.1.1",
    "delay": "^5.0.0",
    "immer": "^9.0.1",
    "js-cookie": "^3.0.0",
    "lodash": "^4.17.21",
    "parcel": "^2.7.0",
    "postcss": "^8.2.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "secure-e2ee": "^0.4.0",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@parcel/reporter-bundle-analyzer": "2.5.0",
